<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jmdb.delegateapp" version="0.0.20" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>The Delegate App</name>
    <description>
        Delegate App for conferences
    </description>
    <author email="james@jmdbsoftware.com" href="http://jmdbsoftware.com">
				James Rooney
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="geo:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="26" />
</widget>
